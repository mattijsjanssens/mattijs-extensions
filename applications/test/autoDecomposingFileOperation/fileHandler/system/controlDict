FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs    ("libautoDecomposingFileOperation.so");

OptimisationSwitches
{
    fileHandler autoReconstructing;
}

DebugSwitches
{
    autoReconstructing 1;
}


application     icoUncoupledKinematicParcelFoam;

startFrom       latestTime;

startTime       0;

stopAt          writeNow;

endTime         0.25;

deltaT          5e-5;

writeControl    runTime;

writeInterval   0.05;

purgeWrite      0;

writeFormat     binary;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

