FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p.orig;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    left1
    {
        type            zeroGradient;
    }
    right1
    {
        type            cyclicAMI;
        value           uniform 0;
    }
    walls1
    {
        type            zeroGradient;
    }
    left2
    {
        type            cyclicAMI;
        value           uniform 0;
    }
    right2
    {
        type            uniformFixedValue;
        uniformValue    constant 0;
        value           uniform 0;
    }
    walls2
    {
        type            zeroGradient;
    }
    lift
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


