FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      snappyHexMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


file motorBike.obj;

geometry
{
    ${file}
    {
        type triSurfaceMesh;
        name motorBike;
    }
};


motorBike.obj 10;

var motorBike\.obj;

b ${$var};



