// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains  100;

method  ptscotch2;
scotch2Coeffs
{
    domains (25);
    domainWeights (0.01);
}
scotchCoeffs
{
    $scotch2Coeffs;
}


