FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [m s^-1];

internalField   uniform (0 0 0);

boundaryField
{
    movingWall
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    fixedWalls
    {
        type            noSlip;
    }
    frontAndBack
    {
        type            empty;
    }
    procBoundary0to2
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary0to3
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


